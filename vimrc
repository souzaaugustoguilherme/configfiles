syntax enable
colorscheme industry

set number relativenumber
set nocompatible
set visualbell

set hlsearch
set smartcase
set ignorecase
set incsearch

set autoindent
set smartindent
set smarttab
set shiftwidth=4
set softtabstop=4

set ruler
set undolevels=1000
set backspace=indent,eol,start

let python_highlight_all = 1
